    "cross-var": "^1.1.0",
    "css-loader": "^7.1.1",
    "dotenv-cli": "^7.4.1",
    "dotenv-webpack": "^8.1.0",
    "html-critical-webpack-plugin": "^2.1.0",
    "mini-css-extract-plugin": "^2.9.0",
    "mkdirp": "^3.0.1",
    "nodemon": "^3.1.0",
    "postcss": "^8.4.38",
    "postcss-loader": "^8.1.1",
    "rimraf": "^5.0.5",
    "sharp": "^0.33.3",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "typescript-loadable-components-plugin": "^1.0.3",
    "wait-on": "^7.2.0",
    "webpack": "^5.91.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^5.10.0",
    "webpack-node-externals": "^3.0.0"
  },
  "overrides": {
    "typescript": "^5.4.5"
  },
  "cracoConfig": "craco.config.ts",
  "reactSnap": {
    "inlineCss": true
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
