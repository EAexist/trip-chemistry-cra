    "webpack-merge": "^5.10.0",
    "webpack-node-externals": "^3.0.0"
  },
  "overrides": {
    "typescript": "^5.4.3"
  },
  "cracoConfig": "craco.config.ts",
  "reactSnap": {
    "inlineCss": true
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
