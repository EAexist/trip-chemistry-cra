{
  "name": "trip-chemistry-v2",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "scripts": {
    "build-client": "webpack --config webpack.client.cjs",
    "build-lib": "babel -d lib --extensions \".ts,.tsx\" ./src",
    "build-server": "webpack --config webpack.server.cjs",
    "build-dev": "set NODE_ENV=development rimraf ./dist ./lib && npm run build-client && npm run build-server && npm run build-lib",
    "build:client": "react-scripts build",
    "build:dev": "set NODE_ENV=development && webpack --config webpack.config.cjs --mode=development",
    "build:prerender": "webpack --config webpack.config.prerender.cjs --mode=production",
    "clean": "find ./dist -maxdepth 1 -type f -not -name name -delete",
    "eject": "react-scripts eject",
    "postBuild": "react-snap",
    "start:client": "react-scripts start",
    "start:client:prod": "serve -s build",
    "build": "craco build",
    "start": "craco build && concurrently \"serve -s build\" \"wait-on -c wait-on.config.json http://localhost:3000/home && npm run lighthouse\" ",
    "start-dev": "craco start",
    "lighthouse": "lighthouse http://localhost:3000/home --output-path=report/%npm_config_REPORT_NAME%/home.html",
    "test": "react-scripts test"
  },
  "dependencies": {
    "@craco/craco": "^7.1.0",
    "@emotion/css": "^11.11.2",
    "@emotion/react": "^11.11.4",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.11.5",
    "@loadable/component": "^5.16.3",
    "@loadable/server": "^5.16.2",
    "@mui/icons-material": "^5.15.14",
    "@mui/material": "^5.15.14",
    "@reduxjs/toolkit": "^2.2.3",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/react-helmet": "^6.1.11",
    "@types/swiper": "^6.0.0",
    "axios": "^1.6.8",
    "express-static-gzip": "^2.1.7",
    "framer-motion": "^11.0.24",
    "http-status-codes": "^2.3.0",
    "react": "^18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^2.0.4",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.3",
    "react-scripts": "^5.0.1",
    "react-snap": "^1.23.0",
    "serve-static": "^1.15.0",
    "swiper": "^11.1.0",
    "typescript": "^5.4.3",
    "web-vitals": "^3.5.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.3",
    "@babel/preset-env": "^7.24.3",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@loadable/babel-plugin": "^5.16.1",
    "@loadable/webpack-plugin": "^5.15.2",
    "@prerenderer/renderer-puppeteer": "^1.2.3",
    "@prerenderer/webpack-plugin": "^5.3.9",
    "@types/express": "^4.17.21",
    "@types/google.maps": "^3.55.5",
    "@types/loadable__component": "^5.13.9",
    "@types/loadable__server": "^5.12.10",
    "@types/mkdirp": "^2.0.0",
    "@types/node": "^20.12.2",
    "@types/react": "^18.2.73",
    "@types/react-dom": "^18.2.23",
    "@types/react-router-dom": "^5.3.3",
    "@types/webpack": "^5.28.5",
    "@types/webpack-env": "^1.18.4",
    "babel-loader": "^9.1.3",
    "babel-plugin-import": "^1.13.8",
    "brotli": "^1.3.3",
    "brotli-webpack-plugin": "^1.1.0",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^8.2.2",
    "copy-webpack-plugin": "^12.0.2",
    "core-js": "^3.36.1",
    "cross-var": "^1.1.0",
    "css-loader": "^6.10.0",
    "dotenv-cli": "^7.4.1",
    "dotenv-webpack": "^8.1.0",
    "html-critical-webpack-plugin": "^2.1.0",
    "mini-css-extract-plugin": "^2.8.1",
    "mkdirp": "^3.0.1",
    "nodemon": "^3.1.0",
    "postcss": "^8.4.38",
    "postcss-loader": "^8.1.1",
    "rimraf": "^5.0.5",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.1",
    "typescript-loadable-components-plugin": "^1.0.3",
    "wait-on": "^7.2.0",
