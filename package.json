{
  "name": "trip-chemistry-v2",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "scripts": {
    "build:client": "react-scripts build",
    "build:dev": "set NODE_ENV=development && webpack --config webpack.config.cjs --mode=development",
    "build:prerender": "webpack --config webpack.config.prerender.cjs --mode=production",
    "clean": "find ./dist -maxdepth 1 -type f -not -name name -delete",
    "eject": "react-scripts eject",
    "postBuild": "react-snap",
    "start:client": "react-scripts start",
    "start:client:prod": "serve -s build",
    "build": "craco build",
    "build-dev": "cross-env NODE_ENV=development FAST_REFRESH=false craco build",
    "start-dev": "craco start",
    "start": "concurrently \"serve -s build\" \"wait-on -c wait-on.config.json http://localhost:3000/home && npm run lighthouse\" ",
    "build-client": "rimraf ./dist/js && npm run build --SSR=true --TARGET=web",
    "build-client-dev": "rimraf ./dist/js && cross-env NODE_ENV=development FAST_REFRESH=false npm run build --SSR=true --TARGET=web",
    "build-server": "rimraf ./dist/node && npm run build --SSR=true --TARGET=node",
    "build-server-dev": "rimraf ./dist/js && cross-env NODE_ENV=development FAST_REFRESH=false npm run build --SSR=true --TARGET=node",
    "build-server_": "rimraf ./dist/node && webpack --config webpack.server.js --SSR=true --TARGET=node",
    "build-lib": "rimraf ./lib && babel -d lib --extensions \".ts,.tsx\" ./src",
    "build-ssr": "npm run build-server ; npm run build-client ; npm run build-lib",
    "build-ssr-dev": "npm run build-client-dev ; npm run build-lib",
    "start-ssr": "node lib/server/index.js",
    "start-ssr-dev": "cross-env NODE_ENV=development node lib/server/index.js",
    "lighthouse": "lighthouse http://localhost:3000/home --output-path=report/%npm_config_REPORT_NAME%/home.html",
    "test": "react-scripts test"
  },
  "dependencies": {
    "@craco/craco": "^7.1.0",
    "@emotion/css": "^11.11.2",
    "@emotion/react": "^11.11.4",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.11.5",
    "@googlemaps/react-wrapper": "^1.1.35",
    "@loadable/component": "^5.16.3",
    "@loadable/server": "^5.16.2",
    "@mui/icons-material": "^5.15.15",
    "@mui/material": "^5.15.15",
    "@mui/x-date-pickers": "^7.6.1",
    "@reduxjs/toolkit": "^2.2.3",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.2",
    "@testing-library/user-event": "^14.5.2",
    "@vis.gl/react-google-maps": "^1.1.0",
    "axios": "^1.6.8",
    "dayjs": "^1.11.11",
    "express-static-gzip": "^2.1.7",
    "framer-motion": "^11.1.3",
    "http-proxy-middleware": "^3.0.0",
    "http-status-codes": "^2.3.0",
    "react": "^18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.4",
    "react-redux": "^9.1.1",
    "react-router-dom": "^6.22.3",
    "react-scripts": "^5.0.1",
    "swiper": "^11.1.1",
    "typescript": "^5.4.5",
    "web-vitals": "^3.5.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@loadable/babel-plugin": "^5.16.1",
    "@loadable/webpack-plugin": "^5.15.2",
    "@prerenderer/renderer-puppeteer": "^1.2.3",
    "@prerenderer/webpack-plugin": "^5.3.9",
    "@types/brotli-webpack-plugin": "^1.1.5",
    "@types/express": "^4.17.21",
    "@types/google.maps": "^3.55.7",
    "@types/jest": "^29.5.12",
    "@types/loadable__component": "^5.13.9",
    "@types/loadable__server": "^5.12.10",
    "@types/mkdirp": "^2.0.0",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@types/react-router-dom": "^5.3.3",
    "@types/swiper": "^6.0.0",
    "@types/webpack": "^5.28.5",
    "@types/webpack-env": "^1.18.4",
    "@types/webpack-node-externals": "^3.0.4",
    "babel-loader": "^9.1.3",
    "babel-plugin-import": "^1.13.8",
